   "source": [
    "# Group by country code and calculate median discounting percent and actual price\n",
    "discount_country = products.groupby('country_code')[['discounting_percent', 'actual_price']].median()\n",
    "discount_country = discount_country.reset_index()\n",
    "discount_country"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 59,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merge products and world dataset for Choropleth map\n",
    "products_geo = pd.merge(world, discount_country, left_on='iso_a3', right_on='country_code', how='left')\n",
    "products_geo['discounting_percent'] = products_geo['discounting_percent'].fillna(0)\n",
    "products_geo['actual_price'] = products_geo['actual_price'].fillna(0)"
   ]
