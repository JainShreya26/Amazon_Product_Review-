   ],
   "source": [
    "# Check for null values\n",
    "products_inital.isnull().sum()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Drop rows with null values\n",
    "products_inital = products_inital.dropna(subset=['actual_price','discount_price'])"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Creating `Discount` related columns "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Make column with discount net value and discounting percent \n",
    "products_inital['discount_value'] = products_inital['actual_price'] - products_inital['discount_price']\n",
    "products_inital['discounting_percent'] = 1 - products_inital['discount_price']/products_inital['actual_price']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0              Lloyd\n",
      "1                 LG\n",
      "2                 LG\n",
      "3                 LG\n",
      "4            Carrier\n",
      "             ...    \n",
      "551579            BE\n",
      "551580        Adidas\n",
      "551581       Redwolf\n",
      "551582       Redwolf\n",
      "551584    Mothercare\n",
      "Name: brand, Length: 334963, dtype: object\n"
     ]
    }
   ],
   "source": [
    "products_inital['brand'] = products_inital['name'].str.split(' ').str[0]\n",
    "print(products_inital['brand'])\n",
    "products_inital['brand'] = products_inital['brand'].str.lower().str.capitalize()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merge all the dataframes\n",
    "products = pd.merge(products_inital, mcap_revenue, on='brand', how='inner')"
   ]
  },
